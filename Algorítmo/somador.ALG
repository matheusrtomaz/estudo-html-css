algoritmo "somador"
var
   cont, soma, numero : inteiro
inicio
      cont <- 1
      soma <- 0
      enquanto (cont <= 5) faca
      escreva ("Digite o ", cont, "o. valor: ")
      leia (numero)
      soma <- soma + numero
      cont <- cont + 1
      fimenquanto
      escreval ("A soma de todos os valores foi ", soma)
fimalgoritmo